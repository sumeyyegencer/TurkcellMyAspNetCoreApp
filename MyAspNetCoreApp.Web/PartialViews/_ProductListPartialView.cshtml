@using MyAspNetCoreApp.Web.ViewModels
@model ProductListPartialViewModel


<table class="table table-hover table-info table-bordered">
    <thead>

        <tr>
            <th>Id</th>
            <th>İsim</th>
            <th>Fiyat</th>
            <th>Stok</th>

        </tr>

    </thead>
    <tbody>

        @foreach(var item in Model.Products)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Price</td>
                <td>@item.Stok</td>
            </tr>
        }


    </tbody>
</table>